
set terminal png

set title "Directory 1 & 2, unison sync between nodes line topography"

set xlabel "Time (Seconds)"
set ylabel "Data transferred (MB)"

set key bottom right

set style data linespoints

plot "< cat nodeA-data-1 nodeB-data-1 nodeC-data-1 nodeD-data-1 | sort -n" using 1:2 title 'Directory 1', \
"< cat nodeA-data-2 nodeB-data-2 nodeC-data-2 nodeD-data-2 | sort -n" using 1:2 title 'Directory 2'
